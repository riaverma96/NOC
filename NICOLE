                var i = 0;

                setPoints(map, jsonData, i);

                function determineColor(jsonData, jsonData2, i) {
                    var count = 0;
                    for (var j = 0; j < 106; j++) {
                      var thislatitude = jsonData2.geoResult[j].xcoordinate;
                      var thislongitude = jsonData2.geoResult[j].ycoordinate;
                      if ((Math.abs(thislatitude - jsonData.geoResult[i].latitude) < .1) ||
                        (Math.abs(thislongitude - jsonData.geoResult[i].longitude) < .1)) {
                        count++;
                      }
                    }
                    return count;
                }

                function setPoints(map, jsonData, i) { //longitude, latitude

                    while (i < 13) {
                        var localLatitude = jsonData.geoResult[i].latitude;
                        var localLongitude = jsonData.geoResult[i].longitude;
                        if (determineColor(jsonData, jsonData2, i) <= 1) {
                          tomtom.Marker.DEFAULT_MARKER_OPTIONS = {
                          iconUrl: "hotelgreen.png",
                          iconSize: [32, 37],
                          iconAnchor: [ 17, 34 ]
                          };
                        } else if (determineColor(jsonData, jsonData2, i) >= 4) {
                          tomtom.Marker.DEFAULT_MARKER_OPTIONS = {
                          iconUrl: "hotelred.png",
                          iconSize: [32, 37],
                          iconAnchor: [ 17, 34 ]
                          };
                        } else {
                          tomtom.Marker.DEFAULT_MARKER_OPTIONS = {
                          iconUrl: "hotelyellow.png",
                          iconSize: [32, 37],
                          iconAnchor: [ 17, 34 ]
                          };
                        }
                        var marker = new tomtom.Marker([localLatitude, localLongitude]);
                        map.addLayer(marker);
                        i++;
                    }
                }
            };


        </script>

